{% extends 'cbeamd/base.django' %}

{% load bootstrap3 %}

{% block content %}

<form action="/login?next={{redirect_to}}" method="post" class="form">
    {% csrf_token %}
    {% bootstrap_form form %}
    {% buttons %}
        <button type="submit" class="btn btn-primary">
            {% bootstrap_icon "star" %} Login
        </button>
    {% endbuttons %}
</form>

{% endblock content %}
